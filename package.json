{
  "name": "react_boilerplate",
  "version": "0.0.2",
  "private": true,
  "description": "Quick boilerplate with React, redux, webpack( with HMR) and redux dev tool ",
  "main": "index.html",
  "repository": "git@github.com:ankitsin/react_boilerplate.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dashboard -- webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 6060",
    "build": "rm -rf dist && mkdir dist && cp -r src/assets dist/assets && NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors",
    "lint": "./node_modules/.bin/eslint --config=.eslintrc.json ./**/*.{jsx,js}",
    "polish": "npm run lint -s -- --fix"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "axios": "0.15.2",
    "babel-plugin-antd": "~0.4.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "normalize": "^0.3.1",
    "prop-types": "^15.6.0",
    "qs": "6.3.0",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "react-redux": "^4.4.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-config-standard": "^6.1.0",
    "eslint-config-standard-jsx": "^3.1.0",
    "eslint-config-standard-react": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^7.5.0",
    "eslint-plugin-standard": "2.0.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^1.3.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.19.0",
    "url-loader": "~0.5.7",
    "webpack": "^3.8.1",
    "webpack-dashboard": "1.0.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4"
  },
